  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Current File (TS)",
      "type": "node",
      "request": "launch",
      "runtimeArgs": ["ts-node", "${file}"],
      "runtimeExecutable": "npx",
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal",
      "outFiles": ["!**/node_modules/**"]
    }
  ]
}
